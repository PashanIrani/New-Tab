{
    "name": "New Tab",
    "version": "3.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["scripts/extenstion.js"]
    },
    "browser_action": {
        "default_icon": "images/icon.png"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }

}
